{
  "icc": {{ docker_icc }},
  "ipv6": {{ docker_ipv6 }},
  "live-restore": {{ docker_live_restore }},
  "log-driver": "{{ docker_log_driver }}",
  "log-level": "{{ docker_log_level }}",
  "log-opts": { "gelf-address": "udp://{{ docker_syslog_server }}:{{ docker_syslog_port }}" },
  "registry-mirrors": ["{{ docker_registry_mirrors }}"],
  "metrics-addr": "0.0.0.0:{{ docker_metrics_port }}",
  "dns": ["{{ docker_dns | join('", "') }}"],
{% if docker_metrics_port is defined %}
  "experimental": true,
{% endif %}
  "userland-proxy": {{ docker_userland_proxy }}
}
